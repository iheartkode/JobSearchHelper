<%= form_for Job.new do |f| %>
<%= f.text_field :company, :placeholder => "Company Name" %> <br>
<%= f.text_field :contact, :placeholder => "Contact" %> <br>
<%= f.text_field :phone, :placeholder => "Phone Number" %> <br>
<%= f.text_field :location, :placeholder => "Location" %> <br>
<%= f.text_area :notes, :placeholder => "Notes" %> <br>
 <%= f.submit %>
<% end %>
